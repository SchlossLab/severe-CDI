cp data/plate52_raw/KR01326*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR01337*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR02742*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR02925*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR03232*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR03437*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR03456*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR03652*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR03681*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR03834*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR03922*.fastq.gz data/combine_test_raw/temp

cd data/combine_test_raw/temp
#Add a 3 before the KR# to indicate 3rd  Miseq run for samples
rename 'KR' 3KR *.gz
cd ../../..
