cp data/reseq_repeat_raw/KR02742*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR02925*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR03232*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR03437*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR03456*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR03652*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR03681*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR03834*.fastq.gz data/combine_test_raw
cp data/reseq_repeat_raw/KR03922*.fastq.gz data/combine_test_raw

cd data/combine_test_raw
#Add a 2 before the KR# to indicate 2nd Miseq run for samples
rename 'KR' 2KR *.gz

cd ../..

#Copy samples from plate_52 MiSeq run
cp data/plate52_raw/KR00041*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR00090*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR00218*.fastq.gz data/combine_test_raw/temp
cp data/plate52_raw/KR00541*.fastq.gz data/combine_test_raw/temp
